package forSlick2D;
import org.newdawn.slick.GameContainer;import org.newdawn.slick.Graphics;//import org.newdawn.slick.geom.Rectangle;import org.newdawn.slick.geom.Shape;import org.newdawn.slick.state.StateBasedGame;
//GameObject는 메인에서의 오브젝트의 증가를 클래스 내부에서 알아서 처리하도록 하기 위해abstract public class GameObject {
 protected BasicScene scene; protected float posX, posY; protected int tag; protected String name;  protected int zorder; protected int _zorder;  protected Shape rect;  public GameObject(){
  this.zorder = 0;  this.name = "";  this.tag = 0; } public GameObject(BasicScene scene){    this();  this.scene = scene; }  public GameObject(BasicScene scene, float posX, float posY, int tag){    this(scene);  this.posX = posX;  this.posY = posY;  this.tag = tag;   }  abstract public void update(); abstract public void render(GameContainer cont, StateBasedGame state, Graphics g); abstract public void remove();  //위치(좌표) public void setPostion(float x, float y){    posX = x;  posY = y; } public float getPositionX(){    return posX; } public float getPositionY(){    return posY; } public void setZOrder(int zorder){    this.zorder = zorder; } public int getZOrder(){    return this.zorder; }  //정보 public void setName(String name){    this.name = name; } public String getName(){    return this.name; } public void setTag(int tag){    this.tag = tag; } public int getTag(){    return this.tag; } public void setRect(Shape rect){
  this.rect = rect; } public Shape getRect(){    return this.rect; }
}
